{
    "summary" : "Check authentication via your Loco API project key",
    "uri" : "auth/verify.json{?key}",
    "httpMethod": "GET",
    "responseClass": "VerifyOutput",
    "parameters": {
        "key" : {
            "description": "Loco project key",
            "required": true,
            "location": "query"
        }
    },
    "errorResponses" : [
        {
            "code" : 401, 
            "phrase" : "API key is not valid"
        }
    ]
}